public class ArrayPract {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
      // Scanner inp = new Scanner(System.in);
       int[] arr = new int[3];
       for(int i=0 ; i<3;i++) {
    	   arr[i] = i;
    	   System.out.println(arr[i]);
       }
       
	}

}
