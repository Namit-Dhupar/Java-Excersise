import java.util.Scanner;
public class Pract {
	 public static void main(String []argh){
	        Scanner in = new Scanner(System.in);
	        int base =1;
	        String res = "";
	        int t=in.nextInt();
	        for(int i=1;i<=t;i++){
	        int a = in.nextInt();
	        int b = in.nextInt();
	        int n = in.nextInt();
	        int curr_term;
	        curr_term=a;
	        for (int j = 1; j <= n ; j++)  
		    { 
		    curr_term += base * b; 
		    res = curr_term + " ";
		    base = base * 2;
		    System.out.print(res);
		    }
	        a = 0;
	        b = 0;
	        n = 0;
	        }
	        in.close();
	 }
}
